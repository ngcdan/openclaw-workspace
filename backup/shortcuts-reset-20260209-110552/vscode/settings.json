{
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.labelFormat": "short",
  "breadcrumbs.enabled": true,
  "zenMode.hideStatusBar": false,
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.fontSize": 13.5,
  "editor.fontLigatures": true,
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.tabCompletion": "on",
  "editor.autoIndent": "advanced",
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 130,
  "editor.rulers": [
    120
  ],
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.minimap.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "editor.foldingStrategy": "auto",
  "editor.foldingImportsByDefault": true,
  "editor.foldingMaximumRegions": 5000,
  "editor.foldingHighlight": true,
  "editor.accessibilitySupport": "off",
  "editor.suggest.showStatusBar": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    // "**/dist": true,
    "**/.vscode": true,
    "**/.turbo": true,
    "**/build": true,
    "**/bin": true,
    "**/.gradle": true,
    "**/.husky": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true
  },
  "search.exclude": {
    "": true,
    "**.map": true,
    "**.d.ts": true,
    "**/node_modules": true,
    "**/.git/**": true
    // "**/host/**": true,
    // "host/**": true,
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[python]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": null
  },
  "[java]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": null
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    // "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.breaks": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.incsearch": true,
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.easymotionKeys": "hklyuiopnm,qwertzxcvbasdgjf:",
  "vim.easymotionMarkerBackgroundColor": "yellow",
  "vim.easymotionMarkerForegroundColorOneChar": "black",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "\\"
      ],
      "commands": [
        "workbench.action.splitEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "revealInExplorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "k"
      ],
      "after": [
        "/",
        "<C-R>",
        "<C-W>",
        "<CR>"
      ]
    },
    {
      "before": [
        "s"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "2",
        "s"
      ]
    },
    // {
    //   "before": [ ":" ],
    //   "commands": [ "workbench.action.showCommands" ],
    // },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "/"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "r"
      ],
      "commands": [
        "references-view.findReferences"
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "q",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        ";"
      ],
      "after": [
        ":"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<S-tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "after": [
        ">",
        "g",
        "v"
      ]
    },
    {
      "before": [
        "<"
      ],
      "after": [
        "<",
        "g",
        "v"
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": [
    "#a89984"
  ],
  "vim.statusBarColors.insert": "#83a598",
  "vim.statusBarColors.visual": "#fe8019",
  "vim.statusBarColors.visualblock": "#fe8019",
  "vim.statusBarColors.replace": "#689d6a",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-c>": false,
    "<C-x>": false,
    "<C-v>": true,
    "<C-z>": false,
    "<C-y>": false,
    "h": true,
    "j": true,
    "k": true,
    "l": true
  },
  "vim.vimrc.enable": true,
  "vim.disableExtension": false,
  "vim.autoindent": true,
  "vim.smartcase": true,
  "vim.timeout": 1000,
  "vim.statusBarColors.visualline": "#fe8019",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 13.5,
  "terminal.integrated.lineHeight": 1,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.suggest.quickSuggestions": {
    "commands": "on",
    "arguments": "on",
    "unknown": "off"
